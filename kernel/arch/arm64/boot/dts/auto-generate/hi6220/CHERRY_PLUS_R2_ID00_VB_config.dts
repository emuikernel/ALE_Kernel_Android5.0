/dts-v1/;
/* NOTE:
 * The last reserved memory base is used in fastboot
 * Which should be the least reseve value
 */
/include/ "fastboot.dtsi"
/include/ "CHERRY_PLUS_R2_ED00_VB_config.dtsi"
/include/ "CHERRY_PLUS_R2_ID00_VB_pin.dtsi"
/include/ "CHERRY_PLUS_R2_ID00_VB_gpio_logic.dtsi"
/include/ "CHERRY_PLUS_R2_ID00_VB_pmic.dtsi"
/include/ "CHERRY_PLUS_R2_ID00_VB_charger.dtsi"
/ {
	hisi,boardid = <0 7 7 2>;
	hisi,boardname = "CHERRY_PLUS_R2_ID00_VB";
    	hisi,modem_id = <0x3D351D00>;
	hisi,product_name = "CHERRY_PLUS_ID00";

    
    boardinfo{
	factory_product_name = "HL4CHRPLSL11MAK";
	normal_product_name = "Che2-U04";
	};

    amba {
	i2c2: i2c@f7102000 {
			st_lis3dh_acc:acc@18{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<0>;
				negate_y=<1>;
				negate_z=<1>;
				status = "ok";
			};
			rohm_kc023_acc:acc@1E{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<0>;
				negate_y=<1>;
				negate_z=<1>;
				status = "ok";
			};
		};
	};
};
