/dts-v1/;
/include/ "fastboot.dtsi"
/include/ "SHINE_UL10_VA_config.dtsi"
/include/ "SHINE_UL13_VA_pin.dtsi"
/include/ "SHINE_UL13_VA_gpio_logic.dtsi"
/include/ "SHINE_UL13_VA_pmic.dtsi"
/include/ "SHINE_UL13_VA_charger.dtsi"
/ {
    hisi,boardid = <0 7 6 5>;
    hisi,boardname = "SHINE_UL13_VA";
    hisi,modem_id = <0x3D351500>;
    hisi,product_name = "CHM-U23";

    boardinfo{
        factory_product_name = "HD1CHMUMCA";
        normal_product_name = "CHM-U23";
    };
	amba {
	i2c2: i2c@f7102000 {
			st_lis3dh_acc:acc@18{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<1>;
				negate_y=<1>;
				negate_z=<0>;
				status = "ok";
			};
			rohm_kc023_acc:acc@1E{
				axis_map_x=<0>;
				axis_map_y=<1>;
				axis_map_z=<2>;
				negate_x=<1>;
				negate_y=<1>;
				negate_z=<0>;
				status = "ok";
			};
			akm_akm09911:compass@0d{
				compass_layout=<4>;
				status = "ok";
			};
		};
	};
	hi1101-power {
	    wifi_sdio_rw = "SDIO_RW_CHM_U23_FEM.bin";
	    hisi_cfg_ini_file = "/system/etc/hisi_cfg_cherry.ini";
	};
};
