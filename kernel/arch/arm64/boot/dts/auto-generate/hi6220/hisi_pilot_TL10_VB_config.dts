/dts-v1/;
/* NOTE:
 * The last reserved memory base is used in fastboot
 * Which should be the least reseve value
 */

/include/ "./hisi_pilot/hi6220_fb.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_tm_nt35521.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_boe_nt35521.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_cmi_nt35521.dtsi"

/include/ "./hisi_pilot/hi6220_mipi_tm_hx8394.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_cmi_otm1284a.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_yassy_ili9881-3.dtsi"
/include/ "./hisi_pilot/hi6220_mipi_yassy_hx8394.dtsi"

/{
	#address-cells = <1>;
	#size-cells = <1>;
	hisi,boardid = <0 3 0 0>;
	hisi,boardname = "hisi_pilot_TL10_VB";
	chosen {
	    bootargs = "console=ttyAMA0,115200 root=/dev/ram0 vmalloc=384M maxcpus=8 earlyprintk no_console_suspend loglevel=15 initcall_debug=n nn_hiwdt100";
	    linux,initrd-start = <0x0ae00000>;
	    linux,initrd-end =   <0x0b600000>;
	};


	fastboot {
		compatible = "hisilicon,fastboot";
		loglevel = <5>;
		console,baudRate = <115200>;
		console,name = "ttyAMA0";
		gpio-keyup = <24>;
		gpio-keydown = <21>;
		udp = "False";
	};


};
/include/ "skeleton.dtsi"
/include/ "hisi_pilot_TL10_VB_pin.dtsi"
/include/ "hisi_pilot_TL10_VB_charger.dtsi"
/include/ "hisi_pilot_TL10_VB_pmic.dtsi"
/include/ "hisi_pilot_TL10_VB_gpio_logic.dtsi"
/include/ "huawei_bastet.dtsi"
/include/ "./hisi_pilot/hi6220_memory.dtsi"
/include/ "./hisi_pilot/hi6220_clk_stub.dtsi"
/include/ "./hisi_pilot/hi6220.dtsi"
/include/ "./hisi_pilot/hi6220_clk.dtsi"
/include/ "./hisi_pilot/hi6220_gpio.dtsi"
/include/ "./hisi_pilot/hi6220_pinctrl.dtsi"
/include/ "./hisi_pilot/hi6552_pmuclk.dtsi"
/include/ "./hisi_pilot/hi6552_pmic_mntn.dtsi"
/include/ "./hisi_pilot/hi6xxx_mtcmos.dtsi"
/include/ "./hisi_pilot/hi6xxx_pm.dtsi"
/include/ "./hisi_pilot/hisi_coul_drv_test.dtsi"
/include/ "./hisi_pilot/bq_bci_battery.dtsi"
/include/ "./hisi_pilot/test_hisi_hi6xxx_regulator.dtsi"
/include/ "./hisi_pilot/hi6220_tsensor.dtsi"
/include/ "./hisi_pilot/hi6220_vcodec.dtsi"
/include/ "./hisi_pilot/hi6220_camera.dtsi"
/include/ "./hisi_pilot/hi6220_videoconfig.dtsi"
