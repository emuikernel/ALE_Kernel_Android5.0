/include/ "./hisi_pilot/hi6220_mali450.dtsi"
/include/ "./hisi_pilot/hisi_hi6220_audio.dtsi"
/include/ "./hisi_pilot/hi6xxx_ddr3.dtsi"
/include/ "./hisi_pilot/huawei_bluetooth.dtsi"
/include/ "./hisi_pilot/huawei_wifi.dtsi"
/include/ "./hisi_pilot/huawei_gps.dtsi"
/include/ "./hisi_pilot/hisi_hi6220_noc.dtsi"
/include/ "./hisi_pilot/hisi_vibrator.dtsi"
